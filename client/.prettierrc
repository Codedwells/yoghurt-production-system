{
	"singleQuote": true,
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"trailingComma": "none",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"semi": true,
	"plugins": ["prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.tsx",
			"options": {
				"quoteProps": "consistent"
			}
		}
	]
}

